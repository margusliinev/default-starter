{
    "name": "default-starter-api",
    "description": "Default Starter API template",
    "author": "Margus Liinev",
    "version": "1.0.0",
    "homepage": "https://github.com/margusliinev/default-starter",
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "private": true,
    "keywords": [
        "Default Starter",
        "Project Template"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/margusliinev/default-starter"
    },
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "build": "bun run build.ts",
        "start": "bun run dist/index.js",
        "format": "prettier --write .",
        "db:generate": "bunx drizzle-kit generate",
        "db:migrate": "bunx drizzle-kit migrate",
        "db:studio": "bunx drizzle-kit studio",
        "db:check": "bunx drizzle-kit check",
        "db:push": "bunx drizzle-kit push",
        "db:pull": "bunx drizzle-kit pull"
    },
    "dependencies": {
        "@elysiajs/cors": "1.4.0",
        "@elysiajs/cron": "1.4.1",
        "@elysiajs/eden": "1.4.6",
        "@elysiajs/openapi": "1.4.12",
        "@elysiajs/static": "1.4.7",
        "drizzle-kit": "1.0.0-beta.6-4414a19",
        "drizzle-orm": "1.0.0-beta.6-4414a19",
        "drizzle-typebox": "1.0.0-beta.6-4414a19",
        "elysia": "1.4.19"
    },
    "devDependencies": {
        "@types/bun": "1.3.5",
        "prettier": "3.7.4",
        "typescript": "5.9.3"
    }
}
