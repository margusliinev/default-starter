{
    "name": "default-starter-api",
    "description": "Default Starter API template",
    "author": "Margus Liinev",
    "version": "1.0.0",
    "homepage": "https://github.com/margusliinev/default-starter",
    "license": "MIT",
    "type": "module",
    "sideEffects": false,
    "private": true,
    "keywords": [
        "Default Starter",
        "Project Template"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/margusliinev/default-starter"
    },
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "build": "bun run build.ts",
        "start": "./server",
        "format": "prettier --write .",
        "db:generate": "bun --bun drizzle-kit generate --config=./src/database/drizzle.config.ts",
        "db:migrate": "bun --bun drizzle-kit migrate --config=./src/database/drizzle.config.ts",
        "db:studio": "bun --bun drizzle-kit studio --config=./src/database/drizzle.config.ts",
        "db:check": "bun --bun drizzle-kit check --config=./src/database/drizzle.config.ts",
        "db:push": "bun --bun drizzle-kit push --config=./src/database/drizzle.config.ts",
        "db:pull": "bun --bun drizzle-kit pull --config=./src/database/drizzle.config.ts"
    },
    "dependencies": {
        "@elysiajs/cors": "1.4.1",
        "@elysiajs/cron": "1.4.1",
        "@elysiajs/eden": "1.4.6",
        "@elysiajs/openapi": "1.4.13",
        "@elysiajs/static": "1.4.7",
        "drizzle-kit": "1.0.0-beta.8-734e789",
        "drizzle-orm": "1.0.0-beta.8-734e789",
        "drizzle-typebox": "1.0.0-beta.8-734e789",
        "elysia": "1.4.20"
    },
    "devDependencies": {
        "@types/bun": "1.3.5",
        "prettier": "3.7.4",
        "typescript": "5.9.3"
    }
}
