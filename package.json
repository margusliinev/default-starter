{
    "name": "default-starter",
    "version": "0.0.1",
    "description": "Default Starter",
    "author": "Margus Liinev",
    "private": true,
    "license": "MIT",
    "scripts": {
        "dev": "concurrently \"nest start --watch --preserveWatchOutput\" \"cd client && npm run dev\"",
        "build": "cd client && npm install && npm run build && cd .. && npm install && nest build",
        "start": "node dist/main",
        "typecheck": "cd client && npm run typecheck && cd .. && tsc",
        "format": "cd client && npm run format && cd .. && prettier . --write",
        "lint": "cd client && npm run lint && cd .. && eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
        "push": "drizzle-kit push:pg",
        "seed": "ts-node src/db/seed.ts",
        "generate": "drizzle-kit generate:pg",
        "migrate": "ts-node src/db/migrate.ts"
    },
    "dependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schedule": "^4.0.0",
        "@nestjs/serve-static": "^4.0.0",
        "@nestjs/swagger": "^7.1.17",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cloudinary": "^1.41.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "drizzle-orm": "^0.29.2",
        "helmet": "^7.1.0",
        "nestjs-form-data": "^1.9.4",
        "pg": "^8.11.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@swc/cli": "^0.1.63",
        "@swc/core": "^1.3.101",
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.7.5",
        "@types/cookie-parser": "^1.4.6",
        "@types/express": "^4.17.17",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.3.1",
        "@types/pg": "^8.10.9",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "concurrently": "^8.2.2",
        "drizzle-kit": "^0.20.8",
        "eslint": "^8.42.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3"
    }
}
